{% extends 'base.html.twig' %}

{% block title %}President{% endblock %}

{% block body %}
    <h1>President</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ president.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ president.date }}</td>
            </tr>
            <tr>
                <th>Prénom</th>
                <td>{{ president.firstname }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ president.lastname }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_president') }}">Retour à la liste</a>

    <a href="{{ path('asf_president_edit', {'id': president.id}) }}">Editer</a>

    {{ include('asf/president/_delete_form.html.twig') }}
{% endblock %}
