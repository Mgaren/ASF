<form method="post" action="{{ path('asf_salaries_delete', {'id': salary.id}) }}" onsubmit="return confirm('Etez-vous sur de vouloir le supprimer?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ salary.id) }}">
    <button class="btn">Supprimer</button>
</form>
