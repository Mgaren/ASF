<form method="post" action="{{ path('asf_adherant_text_delete', {'id': adherant_text.id}) }}" onsubmit="return confirm('Etez-vous sur de vouloir le supprimer?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ adherant_text.id) }}">
    <button class="btn">Supprimer</button>
</form>
