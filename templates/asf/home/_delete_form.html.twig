<form method="post" action="{{ path('asf_delete', {'id': home_asf.id}) }}" onsubmit="return confirm('Etez-vous sur de vouloir le supprimer?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ home_asf.id) }}">
    <button class="btn">Supprimer</button>
</form>
