{% extends 'base.html.twig' %}

{% block title %}SectionSport{% endblock %}

{% block body %}
    <h1>SectionSport</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ section_sport.id }}</td>
            </tr>
            <tr>
                <th>Description du sport</th>
                <td>{{ section_sport.description1 }}</td>
            </tr>
            <tr>
                <th>Renseignement</th>
                <td>{{ section_sport.description2 }}</td>
            </tr>
            <tr>
                <th>Info complémentaire</th>
                <td>{{ section_sport.description3 }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ section_sport.image }}</td>
            </tr>
            <tr>
                <th>Lien du site</th>
                <td>{{ section_sport.lien }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_section_sport') }}">Retour à la liste</a>

    <a href="{{ path('section_sport_edit', {'id': section_sport.id}) }}">Editer</a>

    {# {{ include('section/section_sport/_delete_form.html.twig') }} #}
{% endblock %}
